// 在有序数据中找到目标数据的位置索引
class Solution {
    public int searchInsert(int[] nums, int target) {
        for(int i=0;i<nums.length;i++){
            if(target==nums[i]){
                return i;
            }else if(target>nums[nums.length-1]){
                return nums.length;
            }else if(target<nums[i]){
                return 0;
            }else if(target>nums[i] && target<nums[i+1]){
                return i+1;
            }
        }
        return 20;
    }
}
